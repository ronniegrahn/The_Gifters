
@model LoginVM
@{
	ViewBag.Title = "Login";
}

					<p class="sign-in-an-register-container-egen">
	<form asp-controller="user" asp-action="login">
							
							<label class="sign-in-and-register-label-egen" asp-for="Username"></label>
							<input class="sign-in-and-register-input-egen" asp-for="Username" />
							<span asp-validation-for="Username"></span>

							<label class="sign-in-and-register-label-egen" asp-for="Password"></label>
							<input class="sign-in-and-register-input-egen" asp-for="Password" />
							<span asp-validation-for="Password"></span>
							<br />
							<input class="button-egen" type="submit" value="Login" />
						</form>
						<div asp-validation-summary=All></div>
						
					</p>
				
